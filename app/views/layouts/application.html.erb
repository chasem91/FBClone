<!DOCTYPE html>
<html>
<head>
  <title>FBClone</title>
  <script src="https://use.fontawesome.com/4af02dddaf.js"></script>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script type="text/javascript">
    window.signupImages = {
      findMoreImage: "<%= image_url("session_page/icons/find_more.png") %>",
      seePhotosImage: "<%= image_url("session_page/icons/see_photos.png") %>",
      shareImage: "<%= image_url("session_page/icons/share.png") %>",
    }
    window.homeProfileImages = {
      profilePicture: "<%= image_url("seeds/max_profile.jpg") %>",
      profileBanner: "<%= image_url("seeds/beach_profile_banner") %>"
    }
  </script>
</head>
<body>

<%= yield %>

</body>
</html>
